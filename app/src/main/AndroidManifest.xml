<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartdevelopers.kandie.kandatabase" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayContact"/>
        <activity android:name=".AllContacts"/>
        <activity android:name=".ActivitySharedPref"/>
        <activity android:name=".DisplayUserSettings"/>
        <activity android:name=".UserSettingActivity"/>
        <activity android:name=".UserBroadcast"/>

        <receiver android:name="MyBroadcastReceiver">
            <intent-filter>
                <action android:name="com.smartdevelopers.kandie.kandatabase.A_CUSTOM_INTENT">
                </action>
            </intent-filter>
        </receiver>
    </application>

</manifest>
